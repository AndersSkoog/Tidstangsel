{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ES6",
    "module": "ES6",
    "moduleDetection": "force",
    "allowJs": true,
    "moduleResolution": "bundler",
    "noEmit": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noPropertyAccessFromIndexSignature": false
  }
}